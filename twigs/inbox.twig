{% extends "main.twig" %}
{% block id %}support{% endblock %}
{% block content %}
    <div id="header">
        <div class="container text-center">
            <div class="row">
                <div class="col-md-12">
                    <h2>
                        <span class="fa fa-envelope-o"></span>
                        Your inbox
                    </h2>
                    <p>
                        Message users you've connected with here.
                    </p>
                </div>
            </div>
        </div>
    </div>
    <div id="browse">
        <div class="container">
            <table class="table table-hover">
                <thead>
                <tr>
                    <th>User</th>
                    <th>Message</th>
                    <th>Unread</th>
                </tr>
                </thead>
                <tbody>
                <tr>
                    <td><b>Admin</b></td>
                    <td>Welcome to the site!</td>
                    <td>*</td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
{% endblock %}